{
  "name": "codelab",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "aws-sdk": "^2.691.0",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.16",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "query-string": "^6.12.1",
    "querystring": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "request": "^2.88.2",
    "serve-favicon": "^2.5.0",
    "slugify": "^1.4.0",
    "uuid": "^8.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:css": "node-sass src/main.scss src/pages/app/App.css -w",
    "server": "nodemon server",
    "launch": "npm-run-all --parallel start server",
    "compile:sass": "node-sass src/main.scss src/pages/app/App.comp.css",
    "concat:css": "concat -o src/pages/app/App.concat.css src/pages/app/icons/basic.css src/pages/app/App.comp.css",
    "prefix:sass": "postcss --use autoprefixer -b 'last 10 versions' src/pages/app/App.concat.css -o src/pages/app/App.prefix.css",
    "compress:css": "node-sass src/pages/app/App.prefix.css src/pages/app/App.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:sass compress:css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.8.2",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  }
}
